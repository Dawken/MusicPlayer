	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,md,html,css}": [
			"prettier --write",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/chroma-js": "2.4.0",
